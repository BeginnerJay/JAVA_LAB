package JAVA_0319;

public class JAVA0319_1 {

	public static void main(String[] args) {
		int sum = 0;
		for (int i=1; i<=100; i++) { 	
			System.out.println(i+"번 학생 성적 처리");
		}
		
		for (int j = 1; j <= 100; j++) {
			sum = sum+j;
		}
		System.out.println("1~100까지의 합" + sum);
		
		for (int a = 0, b = 1; a<5; a++, b = b + 2)
			System.out.printf("a=%d, b=%d\n", a,b);
	}
}
