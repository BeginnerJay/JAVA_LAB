package JAVA_0319;
import java.io.*;
public class JAVA0319_10 {

	public static void main(String[] args) throws Exception {
		int[] data = {13211,2326,352135,46543,522465};
		FileOutputStream out = new FileOutputStream("test.bin");
		for(int i = 0; i < 5; i++)
			out.write(data[i]);
		//out.write(data);
		out.close();
		System.out.println("Write Success");
		

	}

}
