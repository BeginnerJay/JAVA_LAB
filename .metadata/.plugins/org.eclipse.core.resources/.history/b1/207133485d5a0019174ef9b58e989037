package chapter11;

class Human {
	private int age;
	private String name;
	
	Human( ) {}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	void intro() {
		System.out.printf("안녕, %d살 %cd입니다.", age, name);
	}
}


public class Student {

}
