package JAVA_0319;

public class JAVA0319_8 {

	public static void main(String[] args) {
		int i = 1234;
		String s = "computer";
		byte b = 12;
		//숫자랑 문자열은 강제 변환이 불가능하다.
		int num = 0x12345678;
		System.out.println("i = " + i);
		System.out.println("s = " + s);
		b = (byte)i; //캐스트 연산자로 타입을 지정하여 에러를 나지 않게 한다.
		System.out.println("b = " + b);
		
		System.out.printf("num = 0x%08x\n", num);
		num = num >> 16;
		System.out.printf("num = 0x%08x\n", num);
	}

}
