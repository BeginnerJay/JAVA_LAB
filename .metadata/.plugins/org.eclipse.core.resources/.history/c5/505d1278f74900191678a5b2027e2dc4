package JAVA_0319;
import java.io.*;
import java.util.Scanner;
public class JAVA0319_10 {

	public static void main(String[] args) throws Exception {
		Scanner scanner = new Scanner(System.in);
		System.out.println("원문 파일 이름 입력  : ");
		int[] data = {1,2,3,4,5};
		FileOutputStream out = new FileOutputStream("test.bin");
		for(int i = 0; i < 5; i++)
			out.write(data[i]);
		//out.write(data);
		out.close();
		System.out.println("Write Success"); // byte로 써진다.
		

	}

}
