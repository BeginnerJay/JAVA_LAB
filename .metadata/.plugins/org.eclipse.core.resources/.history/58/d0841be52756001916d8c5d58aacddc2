package chapter5;

public class Person {
	String name;
	int height;
	double weight;
	char gender;
	boolean married;
	int kids;
	
	public static void main(String[] args) {
		Person man1 = new Person();
		man1.name = "James";
		man1.gender = 'M';
		man1.married = true;
		man1.kids = 3;
		System.out.println(man1);
		System.out.println(man1.name);
		System.out.println(man1.gender);
		System.out.println(man1.married);
		
	}
}

